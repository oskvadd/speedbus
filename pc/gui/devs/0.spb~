name="Outside camera lamp";
source_width=20;

data = (25,25,25);

// Important! event=0 is simply no event at all, you cant add a event with number 0
spb =
{
  frontend = (  { 
		type  = "button";
		label = "Light On";
		event = 1;
		row   = 0; 
		},{ 
		type  = "button";
		label = "Light Off";
		event = 2;
		row   = 0;
		});

  events = ( { event = 1;
               type = "send";
               data = (0x0A,0x00);},
             { event = 2;
               type = "send";
               data = (0x0A,0x00);}
           );
};
